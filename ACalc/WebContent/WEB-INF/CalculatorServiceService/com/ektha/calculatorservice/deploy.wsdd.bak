<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorServiceService WSDL service -->

  <service name="CalculatorService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://calculatorservice.ektha.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorServiceService"/>
      <parameter name="schemaQualified" value="http://calculatorservice.ektha.com"/>
      <parameter name="wsdlServicePort" value="CalculatorService"/>
      <parameter name="className" value="com.ektha.calculatorservice.CalculatorServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculatorService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://calculatorservice.ektha.com" returnQName="retNS:addReturn" xmlns:retNS="http://calculatorservice.ektha.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculatorservice.ektha.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculatorservice.ektha.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="sub" qname="operNS:sub" xmlns:operNS="http://calculatorservice.ektha.com" returnQName="retNS:subReturn" xmlns:retNS="http://calculatorservice.ektha.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculatorservice.ektha.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculatorservice.ektha.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="add sub"/>

  </service>
</deployment>
